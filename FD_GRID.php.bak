//Validate Method @2-366B005B
    function Validate()
    {
        $Validation = true;
        $this->CCSEventResult = CCGetEvent($this->CCSEvents, "OnValidate", $this);

        for($this->RowNumber = 1; $this->RowNumber <= $this->TotalRows; $this->RowNumber++)
        {
            $this->DataSource->CachedColumns["FD_CODE"] = $this->CachedColumns["FD_CODE"][$this->RowNumber];
            $this->DataSource->CurrentRow = $this->RowNumber;
            $this->FD_GRP_ID->SetText($this->FormParameters["FD_GRP_ID"][$this->RowNumber], $this->RowNumber);
            $this->FD_NME->SetText($this->FormParameters["FD_NME"][$this->RowNumber], $this->RowNumber);
            $this->FD_NMF->SetText($this->FormParameters["FD_NMF"][$this->RowNumber], $this->RowNumber);
            $this->CheckBox_Delete->SetText($this->FormParameters["CheckBox_Delete"][$this->RowNumber], $this->RowNumber);
            if ($this->UpdatedRows >= $this->RowNumber) {
                if(!$this->CheckBox_Delete->Value)
                    $Validation = ($this->ValidateRow() && $Validation);
            }
            else if($this->CheckInsert())
            {
                $Validation = ($this->ValidateRow() && $Validation);
            }
        }
        return (($this->Errors->Count() == 0) && $Validation);
    }
//End Validate Method

//ValidateRow Method @2-AC1861FB
    function ValidateRow()
    {
        global $CCSLocales;
        $this->FD_GRP_ID->Validate();
        $this->FD_NME->Validate();
        $this->FD_NMF->Validate();
        $this->CheckBox_Delete->Validate();
        $this->RowErrors = new clsErrors();
        $this->CCSEventResult = CCGetEvent($this->CCSEvents, "OnValidateRow", $this);
        $errors = "";
        $errors = ComposeStrings($errors, $this->FD_GRP_ID->Errors->ToString());
        $errors = ComposeStrings($errors, $this->FD_NME->Errors->ToString());
        $errors = ComposeStrings($errors, $this->FD_NMF->Errors->ToString());
        $errors = ComposeStrings($errors, $this->CheckBox_Delete->Errors->ToString());
        $this->FD_GRP_ID->Errors->Clear();
        $this->FD_NME->Errors->Clear();
        $this->FD_NMF->Errors->Clear();
        $this->CheckBox_Delete->Errors->Clear();
        $errors = ComposeStrings($errors, $this->RowErrors->ToString());
        $this->RowsErrors[$this->RowNumber] = $errors;
        return $errors != "" ? 0 : 1;
    }
//End ValidateRow Method

//CheckInsert Method @2-922E38CF
    function CheckInsert()
    {
        $filed = false;
        $filed = ($filed || (is_array($this->FormParameters["FD_GRP_ID"][$this->RowNumber]) && count($this->FormParameters["FD_GRP_ID"][$this->RowNumber])) || strlen($this->FormParameters["FD_GRP_ID"][$this->RowNumber]));
        $filed = ($filed || (is_array($this->FormParameters["FD_NME"][$this->RowNumber]) && count($this->FormParameters["FD_NME"][$this->RowNumber])) || strlen($this->FormParameters["FD_NME"][$this->RowNumber]));
        $filed = ($filed || (is_array($this->FormParameters["FD_NMF"][$this->RowNumber]) && count($this->FormParameters["FD_NMF"][$this->RowNumber])) || strlen($this->FormParameters["FD_NMF"][$this->RowNumber]));
        return $filed;
    }
//End CheckInsert Method

//CheckErrors Method @2-F5A3B433
    function CheckErrors()
    {
        $errors = false;
