        $this->DataSource->FD_CODE->SetValue($this->FD_CODE->GetValue(true));
        $this->DataSource->FD_GRP_ID->SetValue($this->FD_GRP_ID->GetValue(true));
        $this->DataSource->FD_NME->SetValue($this->FD_NME->GetValue(true));
        $this->DataSource->FD_NMF->SetValue($this->FD_NMF->GetValue(true));
        $this->DataSource->Insert();
        $errors = "";
        if($this->DataSource->Errors->Count() > 0) {
            $errors = $this->DataSource->Errors->ToString();
            $this->RowsErrors[$this->RowNumber] = $errors;
            $this->DataSource->Errors->Clear();
        }
        return (($this->Errors->Count() == 0) && !strlen($errors));
